<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="UML Trace Magic"
            id="org.eclipse.utm">
      </category>
      <view
            name="UTM Input Output"
            icon="icons/sample.gif"
            category="org.eclipse.utm"
            class="org.eclipse.utm.views.UTMInputOutput"
            id="org.eclipse.utm.views.UTMInputOutput">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="org.eclipse.utm.views.UTMInputOutput"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            id="org.eclipse.utm.importWizards.sampleCategory"
            name="UTM File Import">
      </category>
      <wizard
            category="org.eclipse.utm.importWizards.sampleCategory"
            class="org.eclipse.utm.importWizards.ImportWizard"
            icon="icons/sample.gif"
            id="org.eclipse.utm.importWizards.ImportWizard"
            name="Import File">
         <description>
            Import a file from the local file system into the workspace.
         </description>
      </wizard>
   </extension>

</plugin>
